{
    "name": "build-docs",
    "version": "0.1.0",
    "description": "A tool to generate documentation from JSDoc style comments in your codebase.",
    "main": "index.js",
    "scripts": {
        "test": "echo \"no test\"",
        "BuildDocsTest": "ts-node ./src/index.js targetPath=\"./utils/build-docs/test\"",
        "BuildDocsUserValues": "ts-node ./utils/build-docs/index.js targetPath=\"./Applications/RiskRating/1 UserValues\"",
        "BuildDocsUserEventSubscriptions": "ts-node ./utils/build-docs/index.js targetPath=\"./Applications/RiskRating/2 EventSubscriptions\""
    },
    "keywords": [
        "automation",
        "comments",
        "jsdoc",
        "doc",
        "documentation",
        "javascript",
        "js",
        "wiki",
        "typescript",
        "ts",
        "markdown",
        "md",
        "html"
    ],
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.9",
		"@types/eslint": "^8.44.0",
		"@types/node": "^20.4.1",
		"@types/prettier": "^2.7.3",
		"@types/tailwindcss": "^3.1.0",
		"@types/typescript": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"prettier": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"tailwindscss": "^0.3.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
    }
}
