{
  "name": "build-docs",
  "version": "0.1.4",
  "description": "A tool to generate documentation from JSDoc style comments in your codebase.",
  "type": "commonjs",
  "scripts": {
    "test": "npm run DocsToJsonTest && npm run JsonToUiTest",
    "start": "echo \"No start script specified yet.\" && exit 1",
    "setup": "echo \nNo Setup script specified yet.\" && exit 1",
    "DocsToJsonTest": "ts-node -O '{\"module\": \"commonjs\"}' ./src/index.js targetPath=\"./src/_TEST/\"",
    "JsonToUiTest": "ts-node -O '{\"module\": \"commonjs\"}' './src/lib/JsonToUi/JsonToUi.test.ts'",
    "JsonToUiTestOld": "ts-node './src/lib/JsonToUi/JsonToUi.test.ts'",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts, ."
  },
  "keywords": [
    "automation",
    "comments",
    "jsdoc",
    "doc",
    "documentation",
    "javascript",
    "js",
    "wiki",
    "typescript",
    "ts",
    "markdown",
    "md",
    "html"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
